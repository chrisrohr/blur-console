<% if (cluster.get('can_update')) { %>
  <button class="btn btn-primary" disabled="disabled" data-action="disable">Disable Tables</button>
<% } %>
<table class="table table-striped table-bordered" blur_cluster_id='<%= cluster.get('id') %>' id="active_tables">
  <thead>
    <tr>
      <th class="checkbox-td"><input class="check-all" type="checkbox" disabled="disabled"/></th>
      <th>Name<i class="icon-chevron-up"></i> <i class="icon-chevron-down" ></i></th>
      <th class="sorting_disabled">Comments</th>
      <th>Hosts | Shards<i class="icon-chevron-up"></i> <i class="icon-chevron-down" ></i></th>
      <th>Row Count<i class="icon-chevron-up"></i> <i class="icon-chevron-down" ></i></th>
      <th>Record Count<i class="icon-chevron-up"></i> <i class="icon-chevron-down" ></i></th>
      <th class="sorting_disabled">Info</th>
      <th class="sorting_disabled">Queries / Minute</th>      
    </tr>
  </thead>
  <tbody class="active-table">
  </tbody>
</table>
